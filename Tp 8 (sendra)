#include <iostream>
using namespace std;

int main() 
{
    int A;
    cout << "el usuario es culpable de los siguientes crimenes...? 1-Si 2-No" <<endl;
    cin >> A;
    if (A==1){cout<<"el usuario ira a casa"<<endl;}
    else if (A==2){cout << "el usuario ira a la carsel" << endl;}
    return 0;
}
